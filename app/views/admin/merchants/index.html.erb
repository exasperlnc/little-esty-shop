<h1>Merchant Admin</h1>
<br>
<section id="merchants">
  <h2>Merchants</h2>
  <ul id="merchants-list">
    <% @merchants.each do |merchant| %>
      <div id="merchant-<%= merchant.id %>">
      <li><%= link_to merchant.name, admin_merchant_path(merchant) %></li>
      </div>
    <% end %>
  </ul>
</section>